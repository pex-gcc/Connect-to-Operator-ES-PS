{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "api/EventSink",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "QueueAllEvents",
      "queueName": "pxallevents",
      "connection": "QueueStorageAccount"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "QueueParticipantEvents",
      "queueName": "pxpartevents",
      "connection": "QueueStorageAccount"
    }
  ]
}
