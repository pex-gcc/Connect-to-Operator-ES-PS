{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "functionTimeout": "00:10:00",
  "extensions": {
    "http": {
      "routePrefix": ""
    },
    "queues": {
        "maxPollingInterval": "00:00:01",
        "visibilityTimeout" : "00:00:05",
        "batchSize": 32,
        "maxDequeueCount": 10,
        "newBatchThreshold": 16
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  }
}
